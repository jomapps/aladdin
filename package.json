{
  "name": "aladdin",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "db:clean": "cross-env NODE_OPTIONS=--no-deprecation payload run ./scripts/db/clean.js --",
    "db:seed": "cross-env NODE_OPTIONS=--no-deprecation payload run ./scripts/db/clean.js -- --confirm --drop-db && cross-env NODE_OPTIONS=--no-deprecation payload run src/seed/index.ts",
    "db:backup": "cross-env NODE_OPTIONS=--no-deprecation payload run ./scripts/db/backup.js --",
    "db:restore": "cross-env NODE_OPTIONS=--no-deprecation payload run ./scripts/db/restore.js --",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:e2e:dashboard": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=tests/e2e/dashboard.config.ts",
    "test:e2e:dashboard:ui": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=tests/e2e/dashboard.config.ts --ui",
    "test:e2e:dashboard:headed": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=tests/e2e/dashboard.config.ts --headed",
    "test:e2e:evaluation": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=tests/e2e/evaluation.config.ts",
    "test:e2e:evaluation:ui": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=tests/e2e/evaluation.config.ts --ui",
    "test:e2e:evaluation:headed": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=tests/e2e/evaluation.config.ts --headed",
    "test:e2e:automated-gather": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test tests/e2e/automated-gather.spec.ts",
    "test:e2e:automated-gather:ui": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test tests/e2e/automated-gather.spec.ts --ui",
    "test:e2e:automated-gather:headed": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test tests/e2e/automated-gather.spec.ts --headed",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:celery": "cd celery-redis && ./run-tests.sh",
    "test:celery:coverage": "cd celery-redis && ./run-tests.sh coverage",
    "test:celery:fast": "cd celery-redis && ./run-tests.sh fast"
  },
  "dependencies": {
    "@codebuff/sdk": "^0.3.12",
    "@elevenlabs/elevenlabs-js": "^2.16.0",
    "@fal-ai/serverless-client": "^0.14.0",
    "@hookform/resolvers": "^5.2.2",
    "@payloadcms/db-mongodb": "3.57.0",
    "@payloadcms/next": "3.57.0",
    "@payloadcms/payload-cloud": "3.57.0",
    "@payloadcms/plugin-cloud-storage": "3.57.0",
    "@payloadcms/richtext-lexical": "3.57.0",
    "@payloadcms/storage-s3": "3.57.0",
    "@payloadcms/ui": "3.57.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "axios": "^1.7.9",
    "bullmq": "^5.59.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^3.0.0",
    "dotenv": "16.4.7",
    "fluent-ffmpeg": "^2.1.3",
    "graphql": "^16.8.1",
    "ioredis": "^5.4.2",
    "lucide-react": "^0.544.0",
    "mongodb": "^6.20.0",
    "nanoid": "^5.1.6",
    "neo4j-driver": "^5.28.0",
    "next": "15.4.4",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "payload": "3.57.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-day-picker": "^9.11.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-markdown": "^9.0.1",
    "react-use": "^17.5.0",
    "recharts": "^2.10.0",
    "rehype-highlight": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "sharp": "0.34.2",
    "slugify": "^1.6.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "ws": "^8.18.0",
    "zod": "^3.24.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.4.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver",
      "@tailwindcss/oxide",
      "msgpackr-extract"
    ],
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide",
      "msgpackr-extract"
    ]
  }
}
